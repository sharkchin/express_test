div#home-carousel-1.carousel.slide(data-ride="carousel")
  ol.carousel-indicators
    li.active(data-target="#home-carousel-1",data-slide-to="0")
    li(data-target="#home-carousel-1",data-slide-to="1")
    li(data-target="#home-carousel-1",data-slide-to="2")
  div.carousel-inner
    div.item.active
      img(src="holder.js/900x500/auto/#666:#666",alt="first slide")
      div.container
        div.carousel-caption
          h1 terry's clinic is a wonderful place
          p.lead you will enjoy everything here
          a.btn.btn-large.btn-primary(href="join") Check out
    div.item
      img(src="holder.js/900x500/auto/#666:#666",alt="second slide")
      div.carousel-caption
        p image2
    div.item
      img(src="holder.js/900x500/auto/#666:#666",alt="third slide")
      div.carousel-caption
        p image3
  div.left.carousel-control(href="#home-carousel-1",data-slide="prev")
    span.glyphicon.glyphicon-chevron-left
  div.right.carousel-control(href="#home-carousel-1",data-slide="next")
    span.glyphicon.glyphicon-chevron-right